<section id="main-content">
 <section class="wrapper">
  <div class="row">
    <div class="col-lg-12">
      <section class="panel">
        <header class="panel-heading"> Your Bank Identity Details
          </header>

         <table class="table table-bordered table-striped table-condensed">
            <thead>
              <tr>
                <th>Bank Account No</th>
                <th>Bank Name</th>
                <th>Branch Name</th>
                <th>City</th>
                <th>IFSC Code</th>
                <th>Pancard No</th>
                <th>Passport No</th>
                <th>UID No</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
                <tr>
                  <td><%= @bank.account_no %></td>
                  <td><%= @bank.bank_name%></td>
                  <td><%= @bank.branch_name.titleize%></td>
                  <td><%= @bank.city.titleize %></td>
                  <td><%= @bank.ifsc_code%></td>
                  <td><%= @bank.pancard_no%></td>
                  <td><%= @bank.passport_no %></td>
                  <td><%= @bank.uid_no%></td>
                  <td><%= link_to "Edit",  bank_detail_edit_user_employees_path(current_user,:bankidentity => @bank),class: "btn btn-shadow btn-info btn-xs" %></td>
                </tr>
           
             </tbody>
          </table>
       </section>
    
      </div>
    </div>
  </section>
</section>
</section>
